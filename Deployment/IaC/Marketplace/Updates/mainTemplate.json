{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "functions": [
  ],
  "parameters": {
    "utcNowValue": {
      "type": "string",
      "defaultValue": "[utcNow('yyyyMMDDHHmmssFFFF')]"
    },
    "cssaStorageAccountName": {
      "type": "secureString",
      "defaultValue": "#{CSSA_STORAGE_ACCOUNT_NAME}#"
    },
    "cssaStorageAccountKey": {
      "type": "secureString",
      "defaultValue": "#{CSSA_STORAGE_ACCOUNT_KEY}#"
    },
    "cssaSpAppId": {
      "type": "secureString",
      "defaultValue": "#{CSSA_SP_APP_ID}#"
    },
    "cssaSpSecret": {
      "type": "secureString",
      "defaultValue": "#{CSSA_SP_SECRET}#"
    },
    "cssaTenantId": {
      "type": "secureString",
      "defaultValue": "#{CSSA_TENANT_ID}#"
    },
    "importAllRipaAppsSasUrl": {
      "type": "secureString",
      "defaultValue": "#{__TEMPLATE_VERSION_FORMATTED__-IMPORT-ALLRIPAAPPLICATIONS-PS1-SAS-URL}#"
    },
    "importApimApisSasUrl": {
      "type": "secureString",
      "defaultValue": "#{__TEMPLATE_VERSION_FORMATTED__-IMPORT-APIMAPIS-PSM1-SAS-URL}#"
    },
    "newFunctionHostKeySasUrl": {
      "type": "secureString",
      "defaultValue": "#{__TEMPLATE_VERSION_FORMATTED__-NEW-FUNCTIONHOSTKEY-PSM1-SAS-URL}#"
    },
    "newRipaApimBackendSasUrl": {
      "type": "secureString",
      "defaultValue": "#{__TEMPLATE_VERSION_FORMATTED__-NEW-RIPAAPIMBACKEND-PSM1-SAS-URL}#"
    },
    "domainZipSasUrl": {
      "type": "secureString",
      "defaultValue": "#{__TEMPLATE_VERSION_FORMATTED__-DOMAIN-ZIP-SAS-URL}#"
    },
    "stopZipSasUrl": {
      "type": "secureString",
      "defaultValue": "#{__TEMPLATE_VERSION_FORMATTED__-STOP-ZIP-SAS-URL}#"
    },
    "submissionZipSasUrl": {
      "type": "secureString",
      "defaultValue": "#{__TEMPLATE_VERSION_FORMATTED__-SUBMISSION-ZIP-SAS-URL}#"
    },
    "textanalyticsZipSasUrl": {
      "type": "secureString",
      "defaultValue": "#{__TEMPLATE_VERSION_FORMATTED__-TEXTANALYTICS-ZIP-SAS-URL}#"
    },
    "userprofileZipSasUrl": {
      "type": "secureString",
      "defaultValue": "#{__TEMPLATE_VERSION_FORMATTED__-USERPROFILE-ZIP-SAS-URL}#"
    },
    "uiZipSasUrl": {
      "type": "secureString",
      "defaultValue": "#{__TEMPLATE_VERSION_FORMATTED__-UI-ZIP-SAS-URL}#"
    }
  },
  "variables": {},
  "resources": [
    {
      "type": "Microsoft.Resources/deploymentScripts",
      "apiVersion": "2020-10-01",
      "name": "[concat(toLower(take(uniqueString(parameters('utcNowValue')), 3)), 'cssa-app-update-runonce-deleteme-ifucme-dps')]",
      "location": "[resourceGroup().location]",
      "kind": "AzurePowerShell",
      "dependsOn": [],
      "tags": {},
      "properties": {
        "forceUpdateTag": "newGuid()",
        "containerSettings": {
          "containerGroupName": "[concat(toLower(take(uniqueString(parameters('utcNowValue')), 3)), 'cssa-app-update-runonce-deleteme-ifucme-dps')]"
        },
        "storageAccountSettings": {
          "storageAccountName": "[parameters('cssaStorageAccountName')]",
          "storageAccountKey": "[parameters('cssaStorageAccountKey')]"
        },
        "azPowerShellVersion": "3.0",
        "environmentVariables": [
          {
            "name": "CSSA_SP_APP_ID",
            "secureValue": "[parameters('cssaSpAppId')]"
          },
          {
            "name": "CSSA_SP_SECRET",
            "secureValue": "[parameters('cssaSpSecret')]"
          },
          {
            "name": "CSSA_TENANT_ID",
            "value": "[parameters('cssaTenantId')]"
          },
          {
            "name": "APP_SUBSCRIPTION_ID",
            "value": "[subscription().subscriptionId]"
          },
          {
            "name": "APP_RESOURCE_GROUP_NAME",
            "value": "[resourceGroup().name]"
          },
          {
            "name": "DEPLOY_WEB_CONFIG_JSON",
            "value": "False"
          }
        ],
        "primaryScriptUri": "[parameters('importAllRipaAppsSasUrl')]",
        "supportingScriptUris": [
          "[parameters('importApimApisSasUrl')]",
          "[parameters('newFunctionHostKeySasUrl')]",
          "[parameters('newRipaApimBackendSasUrl')]",
          "[parameters('domainZipSasUrl')]",
          "[parameters('stopZipSasUrl')]",
          "[parameters('submissionZipSasUrl')]",
          "[parameters('textanalyticsZipSasUrl')]",
          "[parameters('userprofileZipSasUrl')]",
          "[parameters('uiZipSasUrl')]"
        ],
        "timeout": "PT4H",
        "cleanupPreference": "OnExpiration",
        "retentionInterval": "PT4H"
      }
    }
  ],
  "outputs": {
  }
}