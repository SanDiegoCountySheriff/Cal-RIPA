{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "queues": {
    "batchSize": 6,
    "maxDequeueCount": 5,
    "maxPollingInterval": 60000,
    "newBatchThreshold": 3,
    "visibilityTimeout": "00:00:30"
  }
}